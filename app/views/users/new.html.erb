<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :nausea %>
  <%= f.number_field :nausea %>
  <%= f.label :happiness %>
  <%= f.number_field :happiness %>
  <%= f.label :height %>
  Height:<%= f.number_field :height %>
  <%= f.label :tickets %>
  Tickets:<%= f.number_field :tickets %>
  ADMIN?<%= f.check_box :admin%><br>
  <%= f.submit 'Create User' %>
<% end %>
